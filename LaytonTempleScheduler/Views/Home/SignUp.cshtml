@{
    ViewData["Title"] = "Sign Up";
}
@model TimeSlot

<p> Select a Date</p>
<button>Previous</button>

@*<input type="hidden" name="timeslotID" value="@Model.TimeSlotID" />*@

<form asp-controller="Home" asp-action="SignUp" method="post">
    <input value="@ViewBag.selectedDate" type="date" name="selectedDate"/>
</form>



<!--Supposed to print the appointment times-->

@foreach (var timeSlot in ViewBag.timeSlots)
{
    <p>@timeSlot.Start</p>
} 

<button>Next</button>








